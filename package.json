{
  "name": "proto-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "executor/apps/*",
    "executor/packages/*",
    "sources",
    "assistant",
    "assistant/packages/*"
  ],
  "scripts": {
    "with-env": "bun run --env-file=.env --",
    "dev": "bun run dev.ts",
    "test": "bun run test:executor && bun run test:assistant",
    "test:executor": "bun run --cwd executor test",
    "test:executor:integration": "bun run --cwd executor test:integration",
    "test:executor:e2e": "bun run --cwd executor test:e2e",
    "test:executor:all": "bun run --cwd executor test:all",
    "test:assistant": "bun run --cwd assistant test",
    "typecheck": "bun run typecheck:workspaces",
    "typecheck:slow": "bun run typecheck:workspaces:slow",
    "typecheck:workspaces": "bun run --filter \"@executor/*\" --filter \"@proto/sources\" --filter \"@assistant/*\" --filter \"@openassistant/reacord\" --if-present typecheck",
    "clean": "bun run --filter '*' --if-present clean && rm -rf node_modules .turbo",
    "knip": "bunx knip --config knip.json",
    "db:clear:executor": "bun with-env bun run executor/scripts/dev/wipe/convex-db.ts",
    "convex:codegen": "bunx --cwd executor convex codegen"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@convex-dev/migrations": "^0.3.1",
    "@convex-dev/stripe": "^0.1.3",
    "@convex-dev/workos-authkit": "^0.1.6",
    "@workos-inc/node": "^8.2.0",
    "agentfs-sdk": "^0.6.0",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.111",
    "jose": "^6.1.0",
    "openapi-typescript": "^7.12.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/bun": "latest",
    "convex-test": "^0.0.41",
    "typescript": "^5.9.3",
    "@typescript/native-preview": "7.0.0-dev.20260216.1"
  }
}
