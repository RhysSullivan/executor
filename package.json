{
  "name": "openassistant",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "with-env": "bun run --env-file=.env --",
    "with-env:prod": "bun run --env-file=.env.production --",
    "prepare": "effect-language-service patch",
    "typecheck": "bun run --filter '@openassistant/core' typecheck && bun run --filter '@openassistant/reacord' typecheck && bun run --filter '@openassistant/server' typecheck",
    "typecheck:tsc": "tsc --build",
    "build": "bun run typecheck",
    "test": "bun test packages/ apps/",
    "test:watch": "bun test --watch packages/ apps/",
    "dev": "bun run dev:server & bun run dev:bot & wait",
    "dev:server": "bun run --filter '@openassistant/server' dev",
    "dev:bot": "bun run --filter '@openassistant/bot' dev"
  },
  "devDependencies": {
    "@effect/language-service": "latest",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "latest",
    "@typescript/native-preview": "7.0.0-dev.20260205.1",
    "typescript": "latest",
    "vitest": "^3.2.0"
  },
  "overrides": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@effect/platform": "latest",
    "@effect/platform-bun": "latest",
    "@effect/rpc": "latest",
    "@effect-atom/atom": "^0.4.11",
    "@effect-atom/atom-react": "^0.4.4",
    "@types/react": "^19.0.0",
    "@types/react-reconciler": "^0.31.0",
    "ai": "^6.0.73",
    "discord.js": "^14.17.3",
    "effect": "latest",
    "react": "^19.0.0",
    "react-reconciler": "^0.31.0",
    "zod": "^4.3.6"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "yaml": "^2.8.2"
  }
}
