{
  "name": "@assistant/bot",
  "private": true,
  "type": "module",
  "scripts": {
    "with-env": "bun run --env-file=../../../.env --",
    "dev": "bun with-env bun --hot src/index.ts",
    "start": "bun with-env bun src/index.ts",
    "typecheck": "bunx --package @typescript/native-preview tsgo --noEmit -p tsconfig.json",
    "typecheck:slow": "bunx tsc --noEmit -p tsconfig.json",
    "clean": "rm -rf node_modules .turbo"
  },
  "dependencies": {
    "@assistant/server": "workspace:*",
    "discord.js": "^14.25.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
