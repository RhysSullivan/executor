{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "ignoreIssues": {
    "executor/packages/core/src/**/*.ts": [
      "types"
    ]
  },
  "workspaces": {
    "executor/packages/database": {
      "entry": [
        "*.ts"
      ],
      "ignore": [
        "_generated/server.d.ts"
      ]
    },
    "executor/packages/core": {
      "entry": [
        "src/function-builders.ts",
        "src/identity.ts",
        "src/mcp-server.ts",
        "src/tool-discovery.ts",
        "src/tool-sources.ts",
        "src/managed-runtime.ts",
        "src/slug.ts",
        "../database/**/*.ts",
        "../../executor.ts"
      ]
    }
  }
}
