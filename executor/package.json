{
  "name": "executor",
  "dependencies": {
    "@vercel/sandbox": "^1.5.0",
    "better-result": "^2.7.0",
    "convex": "^1.31.7",
    "graphql": "^16.12.0"
  },
  "private": true,
  "scripts": {
    "doctor": "bun run executor.ts doctor",
    "doctor:prod": "bun run scripts/prod/doctor-prod.ts",
    "deploy:prod": "bun run scripts/prod/deploy-prod.ts",
    "setup:prod:env": "bun run scripts/prod/setup/prod-env.ts --from-env",
    "setup:prod:cloudflare": "bun run scripts/prod/setup/prod-cloudflare.ts",
    "setup:prod:all": "bun run setup:prod:cloudflare --deploy && bun run setup:prod:env && bun run doctor:prod",
    "iac:install": "bun install --cwd infra",
    "iac:deploy": "bun run --cwd infra deploy",
    "iac:diff": "bun run --cwd infra diff",
    "iac:remove": "bun run --cwd infra remove",
    "up": "bun run executor.ts up",
    "upgrade": "bun run executor.ts upgrade",
    "down": "bun run executor.ts down",
    "backend": "bun run executor.ts backend",
    "convex": "bun run executor.ts backend",
    "web": "bun run executor.ts web",
    "deploy": "bunx convex deploy",
    "codegen": "bunx convex codegen",
    "test:e2e:install": "bun test scripts/e2e/install.e2e.test.ts",
    "test:e2e:install:sandbox": "bun test scripts/e2e/install.sandbox.e2e.test.ts",
    "sandbox:install:session": "bun run scripts/e2e/install.sandbox.session.ts",
    "build:binary": "bun build executor.ts --compile --outfile dist/executor",
    "build:release": "bun run scripts/release/build-release.ts"
  },
  "type": "module"
}
