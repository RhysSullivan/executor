{
  "name": "@openassistant/server",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./routes": "./src/routes.ts"
  },
  "scripts": {
    "with-env": "bun run --env-file=../../.env --",
    "dev": "bun run with-env bun --hot src/index.ts",
    "start": "bun run with-env bun src/index.ts",
    "typecheck": "tsgo --noEmit",
    "test": "bun test src/"
  },
  "dependencies": {
    "@openassistant/core": "workspace:*",
    "@openassistant/tool-gen": "workspace:*",
    "@elysiajs/eden": "latest",
    "elysia": "latest",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
